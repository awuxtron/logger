{
    "compilerOptions": {
        "esModuleInterop": true,
        "lib": [
            "ES2020"
        ],
        "module": "ES2020",
        "moduleResolution": "Node",
        "noImplicitAny": false,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": true,
        "strictNullChecks": true,
        "target": "ES2020",
        "verbatimModuleSyntax": true
    }
}
